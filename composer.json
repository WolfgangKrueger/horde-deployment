{
    "name": "horde/bundle",
    "description": "A base project for a horde installation",
    "type": "project",
    "repositories": {
        "timetool": {
            "type": "vcs",
            "url": "https://git.intern.b1-systems.de/development/horde/timetool.git"
        },
        "lunch": {
            "type": "vcs",
            "url": "https://git.intern.b1-systems.de/development/horde/lunch.git"
        },
        "planer": {
            "type": "vcs",
            "url": "https://git.intern.b1-systems.de/development/horde/planer.git"
        },
        "0": {
            "url": "https://horde-satis.maintaina.com",
            "type": "composer"
        }
    },
    "require": {
        "composer-plugin-api": "^2.0",
        "horde/horde-installer-plugin": "^2 || dev-FRAMEWORK_6_0 || dev-master",
        "horde/horde": "^6 || dev-FRAMEWORK_6_0",
        "horde/routes": "^3 || dev-FRAMEWORK_6_0",
        "horde/hordectl": " * || dev-FRAMEWORK_6_0",
        "pear/console_color2": "^0.1.2",
        "pear/console_table": "^1.3",
        "horde/content": "dev-FRAMEWORK_6_0",
        "horde/kronolith": "dev-FRAMEWORK_6_0",
        "horde/mnemo": "dev-FRAMEWORK_6_0",
        "horde/nag": "dev-FRAMEWORK_6_0",
        "horde/passwd": "dev-FRAMEWORK_6_0",
        "horde/turba": "dev-FRAMEWORK_6_0",
        "horde/imp": "dev-FRAMEWORK_6_0",
        "horde/ingo": "dev-FRAMEWORK_6_0",
        "horde/gollem": "^4 || dev-FRAMEWORK_6_0",
        "horde/timetool": "dev-master || dev-FRAMEWORK_6_0",
        "horde/planer": "dev-refactor || dev-FRAMEWORK_6_0",
        "horde/lunch": "dev-master || dev-FRAMEWORK_6_0",
        "horde/injector": "dev-FRAMEWORK_6_0",
        "horde/core": "dev-FRAMEWORK_6_0",
        "horde/theme-material": "dev-FRAMEWORK_6_0"
    },
    "require-dev": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0 || dev-master"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ralf Lang",
            "email": "lang@b1-systems.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": [
            "horde-library",
            "horde-application"
        ]
    }
}
