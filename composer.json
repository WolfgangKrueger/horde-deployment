{
    "name": "horde/bundle",
    "description": "A base project for a horde installation",
    "type": "project",
    "repositories": {
        "0": {
            "url": "https://horde-satis.maintaina.com",
            "type": "composer"
        }
    },
    "require": {
        "composer-plugin-api": "^1.0 || ^2.0",
        "horde/horde-installer-plugin": "dev-FRAMEWORK_6_0 || dev-master",
        "horde/horde": "^6 || dev-FRAMEWORK_6_0",
        "horde/imp": "^7 || dev-FRAMEWORK_6_0",
        "horde/ingo": "^4 || dev-FRAMEWORK_6_0",
        "horde/kronolith": "^5 || dev-FRAMEWORK_6_0",
        "horde/passwd": "^6 || dev-FRAMEWORK_6_0",
        "horde/nag": "^5 || dev-FRAMEWORK_6_0",
        "horde/mnemo": "^5 || dev-FRAMEWORK_6_0",
        "horde/ansel": "^4 || dev-FRAMEWORK_6_0",
        "horde/routes": "^3 || dev-FRAMEWORK_6_0",
        "horde/hordectl": "dev-master || ^1 || dev-FRAMEWORK_6_0",
        "pear/console_color2": "^0.1.2",
        "pear/console_table": "^1.3"
    },
    "require-dev": {
        "horde/test": "dev-master || ^3 || dev-FRAMEWORK_6_0"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ralf Lang",
            "email": "lang@b1-systems.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-types": [
            "horde-library",
            "horde-application"
        ]
    }
}
